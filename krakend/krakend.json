{
  "version": 2,
  "output_encoding": "json",
  "name": "createCart",
  "port": 8081,
  "endpoints": [
    {
      "endpoint": "/recommender/{movie_name}",
      "method": "GET",
      "extra_config": {
        "github.com/devopsfaith/krakend/proxy": {
          "sequential": true
        }
      },
      "output_encoding": "json",
      "backend": [
        {
          "url_pattern": "/product/get_recommendation_data",
          "encoding": "json",
          "method": "GET",
          "host": [
            "http://product_microservice_ct:500"
          ],
          "disable_host_sanitize": false
        },
        {
          "url_pattern": "get_recommendation/{movie_name}{resp0_overviews}{resp0_titles}",
          "encoding": "json",
          "method": "GET",
          "host": [
            "https://us-central1-psychotechnogcp.cloudfunctions.net/"
          ],
          "disable_host_sanitize": false
        }
      ]
    }
  ]
}


